kelurahan=[
"kampung baru",
"kampung bugis",
"kampung kajanan",
"kampung singaraja",
"kendran",
"liligundi",
"paket agung",
"penarukan"
]

for i, item in enumerate(desa_buleleng):
    print(f"{i+1}. {item}")
    
print()
print("kelurahan")
for i, item in enumerate(kelurahan):
    print(f"{i+1}. {item}")
    
    
    
